) {\n            .treats__container .treat-separator:before {\n                width: 140px\n            }\n        }\n\n        .letters-nugget {\n            width: 100%;\n            overflow: hidden;\n            font-size: 0;\n            border-bottom: 2px solid #DFE1E4;\n            padding-top: 6px;\n            position: relative\n        }\n\n        @media all and (min-width:30em) {\n            .letters-nugget {\n                margin: 0 10px\n            }\n        }\n\n        @media all and (min-width:41.25em) {\n            .letters-nugget {\n                max-width: 620px;\n                margin: 0 auto\n            }\n        }\n\n        @media all and (min-width:46.25em) {\n            .letters-nugget {\n                max-width: 700px;\n                margin-left: 20px\n            }\n        }\n\n        @media all and (min-width:61.25em) {\n            .letters-nugget {\n                max-width: 940px;\n                padding-top: 14px\n            }\n        }\n\n        @media all and (min-width:71.25em) {\n            .letters-nugget {\n                max-width: none;\n                width: 140px;\n                border: 0;\n                padding-top: 3px;\n                overflow: inherit;\n                margin: 0\n            }\n        }\n\n        .letters-nugget h1 {\n            color: #005689;\n            font-weight: 600;\n            font-size: 25px;\n            line-height: 26px\n        }\n\n        @media all and (min-width:81.25em) {\n            .letters-nugget {\n                width: 220px\n            }\n\n            .letters-nugget h1 {\n                font-size: 28px;\n                line-height: 28px\n            }\n        }\n\n        .letters-nugget h1:before {\n            content: '';\n            display: block;\n            height: 8px;\n            background-color: #4bc6df;\n            width: 80px;\n            margin-bottom: 4px\n        }\n\n        .letters-nugget p {\n            marg